{
    "name": "paperg/placelocal",
    "description": "Automated Local Ad Creation and Sales",
    "homepage": "http://www.placelocal.com/",
    "license": "proprietary",
    "require": {
        "monolog/monolog": "1.11.*",
        "slim/slim": "2.4.*",
        "educoder/pest": "1.0.0",
        "pda/pheanstalk": "2.1.*",
        "voodoophp/handlebars": "2.2.*",
        "guzzlehttp/guzzle": "5.0",
        "domnikl/statsd": "~2.0",
        "phpmailer/phpmailer": "~5.2",
        "guzzlehttp/retry-subscriber": "*",
        "facebook/php-ads-sdk": "2.5.*",
        "facebook/php-sdk-v4": "5.1.*",
        "paperg/firehound-blob": "1.1.0",
        "mtdowling/cron-expression": "^1.1",
        "ramsey/uuid": "~3.1",
        "dropbox/dropbox-sdk": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "satooshi/php-coveralls": "dev-master",
        "squizlabs/php_codesniffer": "^2.6"
    },
    "suggest": {
        "ext-apc": "*",
        "ext-curl": "*",
        "ext-gd": "*",
        "ext-geoip": "*",
        "ext-imagick": "*",
        "ext-mcrypt": "*",
        "ext-mysql": "*",
        "ext-oauth": "*",
        "ext-pdo_mysql": "*"
    },
    "autoload": {
        "psr-4": {
            "PaperG\\": ["lib/PaperG/", "test/phpunit/lib/PaperG/"],
            "adExchange\\": ["lib/adExchange/", "test/phpunit/lib/adExchange/"]
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:paperg/firehound-blob"
        }
    ]
}
